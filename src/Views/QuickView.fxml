<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>


<?import javafx.scene.layout.Region?>
<VBox spacing="5.0" styleClass="generalBackground" xmlns="http://javafx.com/javafx/8.0.231" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.QuickViewController">
    <padding>
        <Insets bottom="10.0" left="30.0" right="30.0" top="5.0" />
    </padding>
    <HBox  alignment="CENTER_LEFT"   prefHeight="80.0" prefWidth="1000.0" minHeight="80.0" spacing="10.0"  styleClass="sectionHead" >
        <children>
            <VBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="250.0" spacing="15.0">
                <children>
                    <Label fx:id="lbTitle" text="Equipo" styleClass="titleDark" ></Label>
                    <Label fx:id="lbSubTitle" styleClass="subtitleGrey" text="Listado" />
                </children>
                <opaqueInsets>
                    <Insets top="10.0" />
                </opaqueInsets>
            </VBox>
            <Region prefHeight="1.0" prefWidth="1.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnAdd" styleClass="sectionHeadButton" onAction="#handleClicks" >
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../resources/images/anadirUsuario_64px.png" />
                        </image>
                    </ImageView>
                </graphic>
                <HBox.margin>
                    <Insets right="10.0" />
                </HBox.margin>
            </Button>
        </children>
    </HBox>
    <VBox fx:id="vbContent" spacing="5.0" styleClass="generalBackground">
        <children>
        </children>
    </VBox>
</VBox>


