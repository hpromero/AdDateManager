<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER_LEFT" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.DetailDepartmentController">
   <children>
      <HBox alignment="TOP_LEFT" minHeight="40.0" prefHeight="40.0" prefWidth="1000.0" spacing="20.0">
            <children>
               <Label fx:id="lTitle" styleClass="subtitleGrey" text="Equipo" />
               <Region prefHeight="1.0" prefWidth="1.0" HBox.hgrow="ALWAYS" />
               <Button fx:id="btnDepartment" mnemonicParsing="false" onAction="#handleClicks" styleClass="sectionHeadButton" />
               <Button fx:id="btnDepartment2" mnemonicParsing="false" onAction="#handleClicks" styleClass="sectionHeadButton" />
            </children>
      </HBox>
      <HBox>
         <children>
            <VBox styleClass="detailSection">
               <children>
                  <HBox>
                     <children>
                        <VBox spacing="10.0">
                           <children>
                              <Label fx:id="lMsg" style="-fx-text-fill: red">
                                 <VBox.margin>
                                    <Insets bottom="10.0" />
                                 </VBox.margin>
                              </Label>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Nombre:" />
                                    <TextField fx:id="tfName" prefHeight="26.0" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Asigando a:" />
                                    <ComboBox fx:id="chbAssigned" prefWidth="150.0" styleClass="combo-box-base" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Suplente:" />
                                    <ComboBox fx:id="chbAssigned2" prefWidth="150.0" styleClass="combo-box-base" />
                                 </children>
                              </HBox>
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <padding>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Region prefHeight="1.0" prefWidth="1.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>


   </children>
</VBox>
