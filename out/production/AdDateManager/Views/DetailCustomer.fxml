<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER_LEFT" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.DetailCustomerController">
   <children>
      <HBox alignment="TOP_LEFT" minHeight="40.0" prefHeight="40.0" prefWidth="1000.0" spacing="20.0">
            <children>
               <Label fx:id="lTitle" styleClass="subtitleGrey" text="Equipo" />
               <Region prefHeight="1.0" prefWidth="1.0" HBox.hgrow="ALWAYS" />
               <Button fx:id="btnCustomer" mnemonicParsing="false" onAction="#handleClicks" styleClass="sectionHeadButton" />
               <Button fx:id="btnCustomer2" mnemonicParsing="false" onAction="#handleClicks" styleClass="sectionHeadButton" />
            </children>
      </HBox>
      <HBox>
         <children>
            <VBox styleClass="detailSection">
               <children>
                  <HBox>
                     <children>
                        <VBox spacing="10.0">
                           <children>
                              <Label fx:id="lMsg" style="-fx-text-fill: red">
                                 <VBox.margin>
                                    <Insets bottom="10.0" />
                                 </VBox.margin>
                              </Label>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="DNI:" />
                                    <TextField fx:id="tfDni" prefHeight="26.0" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Nombre:" />
                                    <TextField fx:id="tfName" prefHeight="26.0" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="F.Nacimiento:" />
                                    <DatePicker fx:id="dpBithday" prefHeight="26.0" prefWidth="300.0" styleClass="date-picker" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Género:" />
                                    <ChoiceBox fx:id="chbGender" prefWidth="150.0" styleClass="combo-box-base" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="RGPD:" />
                                    <CheckBox fx:id="cbRgpd" mnemonicParsing="false" text="CheckBox" />
                                 </children>
                              </HBox>
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <padding>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                           </padding>
                        </VBox>
                        <VBox spacing="10.0">
                           <children>
                              <Label>
                                 <VBox.margin>
                                    <Insets bottom="10.0" />
                                 </VBox.margin>
                              </Label>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Tutor legal:" />
                                    <TextField fx:id="tfGuardian" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Email:" />
                                    <TextField fx:id="tfEmail" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Teléfono:" />
                                    <TextField fx:id="tfPhone" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Contacto Alt:" />
                                    <TextField fx:id="tfAltContact" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Telefono:" />
                                    <TextField fx:id="tfAltPhone" prefWidth="300.0" />
                                 </children>
                              </HBox>
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <padding>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
                  <Separator prefWidth="200.0">
                     <padding>
                        <Insets bottom="20.0" />
                     </padding>
                  </Separator>
                  <HBox>
                     <children>
                        <VBox spacing="10.0">
                           <children>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Colegio:" />
                                    <TextField fx:id="tfSchool" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Curso:" />
                                    <TextField fx:id="tfCourse" prefWidth="300.0" />
                                 </children>
                              </HBox>
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <padding>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                           </padding>
                        </VBox>
                        <VBox spacing="10.0">
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <padding>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                           </padding>
                           <children>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Derivado de:" />
                                    <TextField fx:id="tfDerivedFrom" prefWidth="300.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefHeight="26.0" prefWidth="140.0" text="Nos conoce por:" />
                                    <TextField fx:id="tfKnowUsFor" prefWidth="300.0" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="269.0">
                     <children>
                        <VBox spacing="10.0">
                           <children>
                              <HBox>
                                 <children>
                                    <Button fx:id="btPDF" mnemonicParsing="false" onAction="#handleClicks" text="Informe PDF" />
                                 </children>
                              </HBox>
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <padding>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Region prefHeight="1.0" prefWidth="1.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>


   </children>
</VBox>
